ENTRY(reset_handler)

MEMORY
{
	FLASH (rx) : ORIGIN = (0x00000000), LENGTH = 0x4000
	RAM (rwx)  : ORIGIN = (0x1FFF0000), LENGTH = 0x4000
}

SECTIONS
{
	.text :
	{
		KEEP(*(.vtor))
		*(.text*)
		*(.rodata*)
	} >FLASH

	.data :
	{
		*(.data*)
	} > RAM AT>FLASH

	.bss :
	{
		*(.bss*)
		*(COMMON)
	} > RAM
}
